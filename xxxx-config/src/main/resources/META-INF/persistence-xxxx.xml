        <!-- cap-auth-admin start -->
        <class>com.iisigroup.xxxx.auth.model.DefaultUser</class>
        <class>com.iisigroup.xxxx.auth.model.Department</class>
        <class>com.iisigroup.xxxx.auth.model.DefaultRole</class>
        <class>com.iisigroup.xxxx.auth.model.RoleFunction</class>
        <class>com.iisigroup.xxxx.auth.model.UserRole</class>
        <class>com.iisigroup.xxxx.auth.model.DefaultFunction</class>
        <class>com.iisigroup.xxxx.auth.model.PwdLog</class>
